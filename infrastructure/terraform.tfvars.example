# Example Terraform variables file
# Copy this to terraform.tfvars and customize for your environment

# AWS Configuration
aws_region     = "us-east-1"
environment    = "dev"
project_name   = "peptide-tracker"

# Lambda Configuration
lambda_timeout = 30
lambda_memory_size = 256

# AWS Profile (optional - leave empty to use default profile)
aws_profile = "peptide-tracker"