{
  "name": "sessions-validator-lambda",
  "version": "1.0.0",
  "description": "Sessions validator Lambda function for JWT token and session validation",
  "main": "build/index.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "clean": "rm -rf build"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.450.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.7.2",
    "jest": "^30.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>"],
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.d.ts",
      "!**/node_modules/**"
    ]
  },
  "keywords": [
    "aws",
    "lambda",
    "sessions-validator",
    "jwt",
    "authentication",
    "git-deployment"
  ],
  "author": "Peptide Tracker Team",
  "license": "MIT"
}
